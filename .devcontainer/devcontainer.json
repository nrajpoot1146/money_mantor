{
    "name" : "Flutter",
    "image": "ubuntu/latest",
    "features": {
        "ghcr.io/devcontainers/features/ghcr.io/devcontainers/features/docker-in-docker:2" : {},
        "ghcr.io/devcontainers/features/flutter:latest":{}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "Dart-Code.dart-code",
                "Dart-Code.flutter"
            ]
        }
    },
    "forwardPorts": [
        8080
    ],
    "portsAttributes": {
        "8080": {
            "label": "Application port"
        }
    }
}